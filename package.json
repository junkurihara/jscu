    "@typescript-eslint/parser": "7.13.1",
    "can-npm-publish": "1.3.6",
    "cross-env": "7.0.3",
    "esdoc": "1.1.0",
    "esdoc-standard-plugin": "1.0.0",
    "eslint": "9.5.0",
    "jasmine-core": "5.1.2",
    "jest": "29.7.0",
    "karma": "6.4.3",
    "karma-chrome-launcher": "3.2.0",
    "karma-cli": "2.0.0",
    "karma-jasmine": "5.1.0",
    "karma-typescript": "5.5.4",
    "lerna": "8.1.3",
    "ts-jest": "29.1.5",
    "ts-loader": "9.5.1",
