    "docs": "./node_modules/.bin/esdoc -c .esdoc.json",
    "test": "lerna run test",
    "karma": "lerna run karma",
    "karma:window": "lerna run karma:window"
  },
  "devDependencies": {
    "@types/elliptic": "6.4.12",
