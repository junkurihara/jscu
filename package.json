    "@typescript-eslint/parser": "4.16.1",
    "can-npm-publish": "1.3.3",
    "cross-env": "7.0.3",
    "esdoc": "1.1.0",
    "esdoc-standard-plugin": "1.0.0",
    "eslint": "7.21.0",
    "jasmine-core": "3.6.0",
    "jest": "26.6.3",
    "karma": "6.1.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-cli": "2.0.0",
    "karma-jasmine": "4.0.1",
    "karma-typescript": "5.4.0",
    "lerna": "4.0.0",
    "ts-jest": "26.5.3",
    "ts-loader": "8.0.17",
    "typescript": "4.2.3",
