    "@typescript-eslint/parser": "5.28.0",
    "can-npm-publish": "1.3.6",
    "cross-env": "7.0.3",
    "esdoc": "1.1.0",
    "esdoc-standard-plugin": "1.0.0",
    "eslint": "8.17.0",
    "jasmine-core": "4.2.0",
    "jest": "28.1.1",
    "karma": "6.4.0",
    "karma-chrome-launcher": "3.1.1",
    "karma-cli": "2.0.0",
    "karma-jasmine": "5.1.0",
    "karma-typescript": "5.5.3",
    "lerna": "5.1.4",
    "ts-jest": "28.0.5",
    "ts-loader": "9.3.0",
