    "@typescript-eslint/parser": "4.28.5",
    "can-npm-publish": "1.3.5",
    "cross-env": "7.0.3",
    "esdoc": "1.1.0",
    "esdoc-standard-plugin": "1.0.0",
    "eslint": "7.32.0",
    "jasmine-core": "3.8.0",
    "jest": "27.0.6",
    "karma": "6.3.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-cli": "2.0.0",
    "karma-jasmine": "4.0.1",
    "karma-typescript": "5.5.1",
    "lerna": "4.0.0",
    "ts-jest": "27.0.4",
